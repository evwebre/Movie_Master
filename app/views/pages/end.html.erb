
<div class="end-banner">
  <video autoplay muted loop id="myendVideo">
    <source src="<%= video_path "endcard.mp4" %>" type="video/mp4">
  </video>
  <div class="end-content">
    <h1> <%= link_to "The End", root_path, class: "end-content-h1" %> </h1>
  </div>
</div>

<% if !current_user.nil? && current_user.level.number > 2 %>
  <% 3.times { current_user.responses.last.destroy } %>
  <% current_user.level = Level.second %>
  <% current_user.save %>
<% end %>

<script>document.getElementById("myendVideo").playbackRate = 0.35;</script>
